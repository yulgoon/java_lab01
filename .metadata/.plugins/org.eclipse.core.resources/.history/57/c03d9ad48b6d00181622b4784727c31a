package bitop;

public class OperationEx5 {

	public static void main(String[] args) {
		int num = 0B0101;  //5
		 
		System.out.println(num << 2);   
		System.out.println(num >> 2);   
		System.out.println(num >>> 2);  
		System.out.println(num);
		
		num = num << 2;
		System.out.println(num);
	}
}
